{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    // "target": "es2016",
    "target": "es2020",
    "module": "commonjs",
    // "module": "esnext",
    "sourceMap": false,
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
     "rootDirs": ["src"],
    "lib": ["es2020"],
    "moduleResolution": "node",
    "types": ["node"]
  },
  "include": ["src/**/*"],
}
